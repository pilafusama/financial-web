/* import common css here */
body {
  background-color: #f5f5f5;
}
.el-header {
  height: 100% !important;
  text-align: center;
  vertical-align: middle;
  line-height: 12vw;
  background-color: #5077aa;
  color: #ffffff;
  font-size: 6vw;
  font-family: HelveticaNeue, Helvetica, Arial, sans-serif;
  border-bottom-color: #d6d3d3;
  padding: 0 2vh 0 0;
  position: relative;
  display: table !important;
  width: 100%;
}
.back {
  width: 5vw;
  height: 5vw;
  left: 1vh;
  top: 3.5vw;
  position: absolute;
}
.back:focus,
.back:hover {
  background-color: #1f99cd;
}
/* Form CSS */
.el-form {
  height: 100% !important;
  text-align: center;
  font-family: HelveticaNeue, Helvetica, Arial, sans-serif;
  background: #ffffff;
  letter-spacing: 1px;
  overflow: hidden;
  display: table;
  width: 100%;
  margin: auto;
}
/* Button CSS */
.el-btn-disable {
  background-color: #9ebddd;
  color: #ffffff;
  line-height: 4vh;
  font-size: 6vw;
  font-family: HelveticaNeue, Helvetica, Arial, sans-serif;
  margin: 3vh 5vw;
  border-radius: 10px;
}
.el-btn {
  background-color: #2f7dcd;
  color: #ffffff;
  line-height: 4vh;
  font-size: 6vw;
  font-family: HelveticaNeue, Helvetica, Arial, sans-serif;
  margin: 3vh 5vw;
  border-radius: 10px;
}
.el-btn:focus,
.el-btn:hover {
  color: #ffffff;
  border-color: #9ebddd;
  background-color: #9ebddd;
}
.el-button.is-disabled,
.el-button.is-disabled:focus,
.el-button.is-disabled:hover {
  color: #ffffff;
  border-color: #9ebddd;
  background-color: #9ebddd;
}
.el-input__inner {
  border: 0;
  text-align: left;
  padding: 0;
  color: #888888;
  font-size: 5vw;
  font-family: HelveticaNeue, Helvetica, Arial, sans-serif;
  border-bottom: 1px solid #d9d9d9;
}
.el-icon-arrow-right {
  padding-right: 2vh;
}
